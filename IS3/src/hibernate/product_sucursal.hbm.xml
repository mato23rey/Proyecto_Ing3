<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping">
<hibernate-mapping>
	<class name="hibernate.Product_Sucursal" table="product_sucursal">
		<composite-id>
			<key-property name="product_id" column="PRODUCT_ID"
				type="integer" />
			<key-property name="sucursal_id" column="SUCURSAL_ID"
				type="integer" />
		</composite-id>
	</class>
</hibernate-mapping>