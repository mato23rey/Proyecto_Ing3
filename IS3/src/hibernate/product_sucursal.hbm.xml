<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping">
<hibernate-mapping>
	<class name="hibernate.Sucursal" table="PHSUCURSAL">
		<id column="ID" name="id" type="integer" />
		<property column="PHARMACY_ID" name="pharmacy_id" type="integer" />
		<property column="NAME" name="name" type="string" />
		<property column="ADDRESS" name="address" type="string" />

	</class>
</hibernate-mapping>